{
    "name": "tsung/nova-user-management",
    "description": "User Management for Laravel Nova version 2",
    "keywords": [
        "laravel",
        "nova"
    ],
    "authors": [
        {
            "name": "anditsung",
            "email": "anditsung@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^7.0|^8.0",
        "laravel/nova": "*",
        "spatie/laravel-permission": "3.16.0",
        "laravel/passport": "^9"
    },
    "autoload": {
        "psr-4": {
            "Tsung\\NovaUserManagement\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tsung\\NovaUserManagement\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
